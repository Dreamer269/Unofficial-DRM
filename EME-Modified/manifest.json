{
  "manifest_version": 3,
  "name": "w1devine",
  "version": "4.10.2557.0",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["content.js", "script.js"],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_icon": "assets/icon.png",
    "default_title": "w1devine",
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "webRequest",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    {
      "resources": ["mod.js", "decrypt.js", "index.html", "modules/*", "assets/redhat.png"],
      "matches": ["<all_urls>"]
    }
  ]
}
